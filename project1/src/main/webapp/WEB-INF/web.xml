<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://java.sun.com/xml/ns/javaee" 
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
version="2.5">
  <display-name>project1</display-name>

	<servlet>
		<servlet-name>ReimbursementServlet</servlet-name>
		<servlet-class>com.revature.controllers.ReimbursementServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReimbursementServlet</servlet-name>
		<url-pattern>/Reimbursements</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>com.revature.controllers.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/Users</url-pattern>
	</servlet-mapping>
	
</web-app>